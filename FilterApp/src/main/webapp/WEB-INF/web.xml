<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>TargetFilter</filter-name>
		<display-name>TargetFilter</display-name>
		<description></description>
		<filter-class>com.rahul.filter.TargetFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>TargetFilter</filter-name>
		<url-pattern>/test</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>TargetServlet</servlet-name>
		<display-name>TargetServlet</display-name>
		<description></description>
		<servlet-class>com.rahul.controller.TargetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TargetServlet</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
</web-app>
